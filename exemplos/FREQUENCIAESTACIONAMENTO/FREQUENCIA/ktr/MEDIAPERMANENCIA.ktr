        <class_source><![CDATA[import org.pentaho.di.core.database.*;
import org.pentaho.di.core.variables.*;
import org.pentaho.di.trans.*;
import java.sql.*;
import java.io.*;
import java.util.*;
import oracle.jdbc.driver.*;

private int outputRowSize;
private int[] paramIndexes;
private DatabaseMeta databaseMeta;
private Database database;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;

        outputRowSize = data.outputRowMeta.size();

		// See which parameters need to be passed...
        //

        String[] parameters = getParameter("PARAMETERS").split(",");

        paramIndexes = new int[parameters.length];
        for (int i=0;i<paramIndexes.length;i++) {
			paramIndexes[i] = getInputRowMeta().indexOfValue(parameters[i]);
    	}
    }

	CallableStatement stmt=null;
	ResultSet rs=null;

    try 
    {
	    // Create a callable statement.
		//
	    stmt = createStatement();

		// Get the results...
        //
        rs = getResultSet(stmt, r);

		Object[] row = database.getRow(rs);
        RowMetaInterface rowMeta = null;
        while(row!=null) {
            if (rowMeta==null) {
			  rowMeta = getInputRowMeta().clone();
              rowMeta.addRowMeta(database.getReturnRowMeta());
            }

		    Object[] outputRow = createOutputRow(r, rowMeta.size());
            for (int i=getInputRowMeta().size();i<rowMeta.size();i++) {
              outputRow[i] = row[i-getInputRowMeta().size()];
  			}

			logRowlevel("Read row: "+rowMeta.getString(outputRow));

			putRow(data.outputRowMeta, outputRow);

			row = database.getRow(rs);
        }
    
    } catch(Exception e) {
		throw new KettleException("Unable to execute stored procedure!", e);
    } finally {
		if (rs!=null) database.closeQuery(rs);
		if (stmt!=null) database.closePreparedStatement(stmt);
	}

	return true;
}


private CallableStatement createStatement() throws Exception {

    // Assemble the query string
    String query = "{call "+getParameter("PROCEDURE")+"(?";
    for (int i=0;i<paramIndexes.length;i++) {
        query+=",?";
    }
    query+=")}";

	logDetailed("Query: '"+query+"'");

	// Create a callable statement.
	//
	CallableStatement stmt = database.getConnection().prepareCall(query);

	return stmt;
}



private ResultSet getResultSet(CallableStatement stmt, Object[] r) throws Exception {

	// Set the parameters...
	//
	for (int i=0;i<paramIndexes.length;i++) {
        int index = paramIndexes[i];
		ValueMetaInterface valueMeta = getInputRowMeta().getValueMeta(index);
        Object valueData = r[index];
        database.setValue(stmt, valueMeta, valueData, i+1);
	}

	// Set the output type to an Oracle Cursor
  	//
    stmt.registerOutParameter(paramIndexes.length+1, OracleTypes.CURSOR);

 	// Execute the stored procedure...
    //
	stmt.execute();

	// Get the results...
    //
    ResultSet rs = (ResultSet)stmt.getObject(paramIndexes.length+1);

	return rs;
}


public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface)
{
    if (parent.initImpl(stepMetaInterface, stepDataInterface)) 
	{
        String dbName = getParameter("CONNECTION");
        databaseMeta = getTransMeta().findDatabase( dbName );
        if (databaseMeta==null) {
           logError("Connection not found: "+dbName);
           return false;
        }
        logDetailed("Using connection "+databaseMeta.getName());
        database = new Database(  databaseMeta );
        try {
          database.connect();
		} catch(Exception e) {
          logError("Unable to connect to database", e);
          return false;
		}
	}

    return true;
}

public void dispose(StepMetaInterface smi, StepDataInterface sdi)
{
    if (database!=null) database.disconnect();
    parent.disposeImpl(smi, sdi);
}
