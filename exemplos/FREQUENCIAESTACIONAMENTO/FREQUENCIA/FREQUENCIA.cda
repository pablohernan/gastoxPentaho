   <DataAccess access="public" connection="dsEmpreendimento" id="dsEmpreendimento" type="sql">
      <Cache duration="3600" enabled="false"/>
      <Columns/>
      <Parameters>
         <Parameter default="${[security:principalName]}" name="user" type="String"/>
      </Parameters>
      <Query><![CDATA[SELECT DISTINCT
    E.COD_EMPREENDIMENTO, LOWER(E.NME_EMPREENDIMENTO) AS NME_EMPREENDIMENTO
FROM
    EMPREENDIMENTO E
ORDER BY
	LOWER(E.NME_EMPREENDIMENTO)]]></Query>
   </DataAccess>
